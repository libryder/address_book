<h1>Listing Users</h1>

<ul>
  <% @users.each do |user| %>
  <li>
    <%= "#{user.first_name} #{user.last_name}" %> - 
    <%= link_to("Edit", edit_user_path(user)) %> - 
    <%= link_to("Show", user_path(user)) %> -  
    <%= link_to "Delete", user, :method => :delete, :class => :destroy %>
  </li>
  <% end %>
</ul>

<%= link_to("New user", new_user_path) %>

